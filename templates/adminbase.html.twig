<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <link rel="icon" type="image/png" href="" />
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
        <title>{% block title %}Julie Rebeyrolle | Admin | {% endblock %}</title>
        <script src="https://code.iconify.design/1/1.0.7/iconify.min.js"></script>
        {{ encore_entry_link_tags('app') }}
        {% block stylesheets %}
        {% endblock %}
    </head>
    <body>
        {% block navbar %} {% include '_includes/_admin_navbar.html.twig' %} {% endblock %}

        {% for category, messages in app.flashes %}
            {% for message in messages %}
                <div class="pt-5">
                <div class="alert alert-{{ category }} text-center rounded-0 mt-5">
                    {{ message }}
                </div>
                </div>
            {% endfor %}
        {% endfor %}

        {% block body %}{% endblock %}

    </body>

    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
    {% endblock %}
</html>
